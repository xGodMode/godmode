{
  "name": "@xgm/godmode",
  "version": "0.0.8",
  "description": "Test smart contracts in GodMode",
  "main": "dist/gm",
  "engines": {
    "node": "12.*"
  },
  "engineStrict": true,
  "files": [
    "/dist"
  ],
  "bin": "bin/godmode.js",
  "scripts": {
    "clean": "rm -R node_modules",
    "dist": "npm run dist:core && npm run dist:bin",
    "dist:bin": "node_modules/typescript/bin/tsc",
    "dist:core": "npm run webpack:prod",
    "lint": "eslint src --ext ts",
    "prepublishOnly": "npm run dist",
    "publish": "npm run release",
    "release": "dotenv release-it -- --only-version --verbose",
    "test": "DEBUG=true mocha -r ts-node/register test/*.ts",
    "webpack": "webpack --mode development",
    "webpack:prod": "webpack --mode production",
    "webpack:watch": "webpack --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xGodMode/godmode.git"
  },
  "keywords": [
    "godmode",
    "ethereum",
    "solidity",
    "xgm",
    "xgmstudio",
    "test",
    "testing",
    "simluate",
    "simulation"
  ],
  "contributors": [
    "Martinet Lee",
    "Val <godmodedevs@gmail.com> (https://twitter.com/codevaltweets)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xGodMode/godmode/issues"
  },
  "homepage": "https://github.com/xGodMode/godmode#readme",
  "dependencies": {
    "@ethersproject/bignumber": "^5.0.13",
    "@xgm/error-codes": "^0.0.5",
    "caip": "^0.9.2",
    "dotenv": "^8.2.0",
    "promise": "^8.1.0",
    "require-directory": "^2.1.1",
    "web3": "^1.3.1",
    "web3-core": "^1.3.4",
    "web3-utils": "^1.3.4",
    "websocket": "^1.0.33",
    "websocket-as-promised": "^1.1.0",
    "ws": "^7.4.3"
  },
  "peerDependencies": {
    "godmode-ganache-core": "git+https://github.com/xGodMode/godmode-ganache-core.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@xgm/contract-library": "0.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "godmode-ganache-core": "git+https://github.com/xGodMode/godmode-ganache-core.git",
    "husky": "^4.3.8",
    "lint-staged": "^10.4.2",
    "mocha": "^8.1.3",
    "prettier": "2.1.2",
    "pretty-quick": "^3.0.2",
    "release-it": "*",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.1.3",
    "webpack-cli": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "npm install",
        "npm run dist"
      ]
    },
    "git": {
      "commitMessage": "release: v${version}"
    },
    "github": {
      "release": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
